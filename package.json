{
  "name": "any-extractor",
  "version": "1.0.3",
  "description": "A universal text extractor for files.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "lint:check": "eslint . --no-error-on-unmatched-pattern",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "text extraction",
    "ocr",
    "llm",
    "image text extraction",
    "file parser"
  ],
  "author": "Pranit Deshmukh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pranit-sh/any-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/pranit-sh/any-extractor/issues"
  },
  "homepage": "https://github.com/pranit-sh/any-extractor#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "THIRD_PARTY_LICENSES.md"
  ],
  "funding": {
    "type": "buymeacoffee",
    "url": "https://www.buymeacoffee.com/pranit.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/concat-stream": "^2.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.14.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^8.0.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "@semantic-release/git": "^10.0.1",
    "@xmldom/xmldom": "^0.9.8",
    "concat-stream": "^2.0.0",
    "file-type-mime": "^0.4.6",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.5.1",
    "pdf-parse": "^1.1.1",
    "tesseract.js": "^6.0.1",
    "undici": "^7.8.0",
    "yauzl": "^3.2.0"
  }
}
